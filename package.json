{
  "name": "ts-fundamentals",
  "version": "1.0.0",
  "description": "Typescript fundamentals",
  "main": "index.js",
  "scripts": {
    "js": "nodemon src/primitives.js",
    "ts": "nodemon --watch 'src/**' --ext 'ts' --exec 'ts-node src/primitives.ts'",
    "classes": "nodemon --watch 'src/**' --ext 'ts' --exec 'ts-node src/classes.ts'",
    "utility": "nodemon --watch 'src/**' --ext 'ts' --exec 'ts-node src/utility.ts'"
  },
  "keywords": [
    "js",
    "ts",
    "Node.js"
  ],
  "author": "Antonio Mata <hildeamp@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^14.14.25",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
